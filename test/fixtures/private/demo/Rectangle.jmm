import io;
import Shape;

class Rectangle extends Shape {
    public int create(int w, int h) {
        int ret;
        ret = this.setX(0);
        ret = ret + this.setY(0);
        ret = ret + this.setWidth(w);
        ret = ret + this.setHeight(h);
        ret = ret + this.setArea(w * h);

        return ret;
    }

    public int create(int w, int h, int x, int y) {
        int ret;

        ret = this.setX(x);
        ret = ret + this.setY(y);
        ret = ret + this.setWidth(w);
        ret = ret + this.setHeight(h);
        ret = ret + this.setArea(w * h);

        return ret;
    }
}

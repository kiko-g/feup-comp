import io;

class Shape {
    int x;
    int y;
    int area;
    int perimeter;
    int width;
    int height;

    public int getX() {
        return x;
    }

    public int getY() { 
        return y;
    }
    
    public void setX(int posX) {
        x = posX;
    }

    public void setY(int posY) { 
        y = posY;
    }

   public void changePos(int x, int y) {
      this.setX(x);
      this.setY(y);
   }

   public void shiftPos(int deltaX, int deltaY) {
      this.changePos(x + deltaX, y + deltaY);
   }

   public void print() {
       io.println(x);
       io.println(y);
       io.println(width);
       io.println(height);
       io.println(area);
       io.println(perimeter);
   }

